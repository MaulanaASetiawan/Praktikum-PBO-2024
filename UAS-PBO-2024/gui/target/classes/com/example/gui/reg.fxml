<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gui.regController">
   <children>
      <VBox layoutX="20.0" layoutY="77.0" prefHeight="185.0" prefWidth="326.0" spacing="10.0">
         <children>
            <Label text="Username">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <TextField fx:id="txtUsername" />
            <Label text="Password">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <PasswordField fx:id="txtPassword" prefHeight="26.0" prefWidth="207.0" promptText="Minimal 5 Karakter" />
            <Label text="Confirm Password">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <PasswordField fx:id="txtConfPassword" promptText="Minimal 5 Karakter" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
         </padding>
      </VBox>
      <Label fx:id="lblJudul" layoutX="86.0" layoutY="34.0" text="Registration">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="btnReg" layoutX="94.0" layoutY="324.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="175.0" text="Registration">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label layoutX="91.0" layoutY="378.0" text="Have an Account?" />
      <Label fx:id="txtLog" layoutX="225.0" layoutY="378.0" text="Sign In" />
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</AnchorPane>
